<?xml version="1.0" encoding="UTF-8"?>
<enunciate
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://enunciate.webcohesion.com/schemas/enunciate-2.12.0.xsd">

    <title>NeuFA-Antrag</title>
    <description package="de.dfg.neufa.antrag.api"/>

    <modules>
        <jaxrs disabled="false"
               groupBy="path"
               path-sort-strategy="depth_first"/>
        <jaxb disabled="true"/>
        <jackson disabled="false"/>
        <jackson1 disabled="true"/>
        <jaxws disabled="true"/>
        <spring-web disabled="true"/>

        <docs disableResourceLinks="false"/>

        <!-- Schaltet die Generierung von Client-Bibliotheken aus: -->
        <c-xml-client disabled="true"/>
        <csharp-xml-client disabled="true"/>
        <java-xml-client disabled="true"/>
        <java-json-client disabled="true"/>
        <javascript-client disabled="true"/>
        <gwt-json-overlay disabled="true"/>
        <obj-c-xml-client disabled="true"/>
        <php-xml-client disabled="true"/>
        <php-json-client disabled="true"/>
        <ruby-json-client disabled="true"/>
        <idl disabled="true"/>
        <swagger disabled="true"/>
    </modules>
</enunciate>
